//Solidity compiler version
pragma solidity ^0.8.0;

//Creating and naming of the contract
contract zkStorage {
    //creating types of uint and name with storedNumber
    uint storedNumber;
    
//Creating a function for store a number
    function storeNumber(uint x) public {
        storedNumber = x;
    }
//Creating a function for get storedNumber
    function retrieveNumber() public view returns (uint) {
        return storedNumber;
    }
}
